<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--namespace需要赋值为Dao类的全限定名称-->
<mapper namespace="com.mycompany.crm.workbench.dao.ActivityDao">
    <!--id需要赋值为Dao类中执行该sql语句的方法名称
        resultType需要赋值为该select语句执行后返回的单行结果的类型的全限定名称
    -->
    <insert id="save" parameterType="com.mycompany.crm.workbench.domain.Activity">
        insert into tbl_activity(
        id,
        owner,
        name,
        startDate,
        endDate,
        cost,
        description,
        createTime,
        createBy,
        editTime,
        editBy

        ) values(
        #{id},
        #{owner},
        #{name},
        #{startDate},
        #{endDate},
        #{cost},
        #{description},
        #{createTime},
        #{createBy},
        #{editTime},
        #{editBy}
        )
    </insert>
</mapper>